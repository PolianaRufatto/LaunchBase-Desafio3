{% extends "layout.njk" %}

    {% block head %}
        <title>Sobre</title>
    {% endblock %}

    {% block content %}
        <div class="wrapper">
            <img src="{{about.avatar_url}}" alt="Logo de {{about.name}}">
            <h1>{{about.aname}}</h1>
            <h2>{{about.description}}</h2>
            <ul>
                <li>Javascript</li>
                <li>NodeJS</li>
                <li>ReactJS</li>
                <li>React Native</li>
            </ul>
        </div>
        <div id="links-footer" class="links">
            {% for link in about.links %}
            <a href="{{link.url}} " target="_blank">{{link.name}}</a>
            {% endfor %}
        </div>
    {% endblock %}